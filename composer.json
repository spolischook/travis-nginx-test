{
    "name": "oro/crm-enterprise",
    "description": "OroCRM Enterprise",
    "homepage": "https://github.com/laboro/crm-enterprise.git",
    "license": "",
    "autoload": {
        "psr-0": {
            "OroCRMPro\\Bundle": "src/",
            "OroPro\\Bundle": "src/"
        }
    },
    "repositories": {
        "composer": {
            "type": "composer",
            "url": "http://packagist.orocrm.com"
        },
        "enterprise": {
            "type": "vcs",
            "url": "https://github.com/orocrm/crm-enterprise.git"
        }
    },
    "require": {
        "php": ">=5.4.4",
        "oro/crm": "1.x-dev"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
