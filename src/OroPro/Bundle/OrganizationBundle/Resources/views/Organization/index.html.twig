{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'organization-grid' %}
{% set pageTitle = 'oro.organization.entity_plural_label'|trans %}

{% block navButtons %}
    {% if resource_granted('oro_organization_create') %}
    <div class="btn-group">
        {{ UI.addButton({
            'path' : path('oropro_organization_create'),
            'entity_label': 'oro.organization.entity_label'|trans,
        }) }}
    </div>
    {% endif %}
{% endblock %}
