{
    "name": "oro/email-bundle",
    "type": "symfony-bundle",
    "description": "BAP Email Bundle",
    "keywords": ["BAP"],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/orocrm/TranslationFormBundle.git"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": ">=2.3,<2.4-dev",
        "a2lix/translation-form-bundle" : "dev-1.x-symfony2.8",
        "oro/attachment-bundle": "dev-master",
        "oro/user-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master",
        "oro/workflow-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\EmailBundle": "" }
    },
    "target-dir": "Oro/Bundle/EmailBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
