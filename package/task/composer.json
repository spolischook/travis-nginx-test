{
    "name": "oro/crm-task-bundle",
    "type": "symfony-bundle",
    "description": "OroCRM Task Bundle",
    "keywords": ["OroCRM"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "oro/platform": "self.version"
    },
    "autoload": {
        "psr-0": { "OroCRM\\Bundle\\TaskBundle": "src/" }
    },
    "target-dir": "src/OroCRM/Bundle/TaskBundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
