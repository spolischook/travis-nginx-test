{
    "name": "oro/crm-task-bundle",
    "type": "symfony-bundle",
    "description": "OroCRM Task Bundle",
    "keywords": ["OroCRM", "Task"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "oro/platform": "self.version"
    },
    "autoload": {
        "psr-0": { "OroCRM\\Bundle\\TaskBundle": "src/" }
    },
    "target-dir": "src/OroCRM/Bundle/TaskBundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
