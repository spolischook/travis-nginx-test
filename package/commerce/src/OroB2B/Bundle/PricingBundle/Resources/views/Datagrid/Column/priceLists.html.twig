{% set priceLists = record.getValue('price_lists') %}
{% if priceLists %}
    {% for priceList in priceLists %}
        <strong>{{ priceList.website.name|capitalize }}</strong>
        {% for websitePriceList in priceList.priceLists  %}
            <div class="text-right"><a href="{{ path('orob2b_pricing_price_list_view', {'id': websitePriceList.id}) }}">{{ websitePriceList.name }}</a></div>
        {% endfor %}
    {% endfor %}
{% endif %}