{
    "name": "oro/crm-hangouts-call-bundle",
    "type": "symfony-bundle",
    "description": "OroCRM Google Hangouts integration",
    "keywords": ["OroCRM", "Google Hangouts", "integration"],
    "homepage": "https://github.com/orocrm/OroCRMHangoutsCallBundle.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "OroCRM\\Bundle\\HangoutsCallBundle": "src/"
        }
    },
    "target-dir": "src/OroCRM/Bundle/HangoutsCallBundle",
    "require": {
        "php": ">=5.5.9",
        "oro/platform": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
