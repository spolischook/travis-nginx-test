{
    "name": "orocrm/contact-bundle",
    "type": "symfony-bundle",
    "description": "Oro CRM Contacts",
    "keywords": ["CRM", "Contacts"],
    "homepage": "https://github.com/orocrm/ContactBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*",
        "friendsofsymfony/rest-bundle": "1.5.0-RC2",
        "nelmio/api-doc-bundle": "dev-master",

        "oro/ui-bundle": "dev-master",
        "oro/user-bundle": "dev-master",
        "oro/address-bundle": "dev-master",
        "oro/email-bundle": "dev-master",
        "oro/dashboard-bundle": "dev-master",
        "orocrm/account-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "target-dir": "OroCRM/Bundle/ContactBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
